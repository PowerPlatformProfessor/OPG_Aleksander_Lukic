<Window x:Class="FitTrack.WorkoutsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitTrack"
        mc:Ignorable="d"
        Title="WorkoutsWindow" Height="450" Width="800">
    <Grid>
        <StackPanel> 
        <Button Content="Lägg till Träningspass" Width="150" Height="30" Margin="10,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Click="AddWorkout_Click"/>
        <Button Content="Ta bort Träningspass" Width="150" Height="30" Margin="170,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Click="AddWorkout_Click"/>
        <ListBox x:Name="WorkoutListBox" Margin="10,50,10,10"/>
        </StackPanel>
    </Grid>
</Window>
